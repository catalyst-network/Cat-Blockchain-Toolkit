{
  "name": "cat-blockchain-toolkit",
  "displayName": "CAT Blockchain Toolkit",
  "description": "Tools for developing Solidity smart contracts in VSCode",
  "publisher": "AtlasCity",
  "icon": "cat-blockchain-toolkit-logo.png",
  "version": "0.0.5",
  "license": "GPL-3.0-or-later",
  "author": "AtlasCity",
  "engines": {
    "vscode": "^1.42.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "Solidity",
    "Catalyst",
    "CAT",
    ".sol",
    "Smart Contract"
  ],
  "homepage": "https://atlascity.io",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/catalyst-network/Cat-Blockchain-Toolkit.git"
  },
  "bugs": {
    "url": "https://github.com/catalyst-network/Cat-Blockchain-Toolkit/issues"
  },
  "contributes": {
    "languages": [
      {
        "id": "solidity",
        "aliases": [
          "Solidity",
          "solidity"
        ],
        "extensions": [
          ".sol"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "solidity",
        "scopeName": "source.solidity",
        "path": "./syntaxes/solidity.json"
      }
    ],
    "snippets": [
      {
        "language": "solidity",
        "path": "./snippets/solidity.json"
      }
    ]
  },
  "scripts": {
    "test": "vscode-tmgrammar-test 'tests/unit/**/*.test.sol'",
    "test:snap": "vscode-tmgrammar-snap 'tests/snap/**/*.sol'",
    "test:snap:update": "vscode-tmgrammar-snap --updateSnapshot 'tests/snap/**/*.sol'"
  },
  "devDependencies": {
    "vscode-tmgrammar-test": "^0.1.3"
  }
}
